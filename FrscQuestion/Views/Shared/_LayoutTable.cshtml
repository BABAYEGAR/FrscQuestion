<script>
    $(document).ready(function() {
        const table = $('#datatable').DataTable({
            dom: 'Bfrtip',
            buttons: [
                {
                    extend: 'copy',
                    text: 'Copy',
                    className: 'btn btn-default',
                    exportOptions: {
                        columns: 'th:not(:last-child)'
                    }
                },
                {
                    extend: 'print',
                    text: 'Print',
                    className: 'btn btn-primary',
                    exportOptions: {
                        columns: 'th:not(:last-child)'
                    }
                }, {
                    extend: 'excel',
                    text: 'Excel',
                    className: 'btn btn-success',
                    exportOptions: {
                        columns: 'th:not(:last-child)'
                    }
                }, {
                    extend: 'csv',
                    text: 'CSV',
                    className: 'btn btn-warning',
                    exportOptions: {
                        columns: 'th:not(:last-child)'
                    }
                },
                {
                    extend: 'pdf',
                    text: 'PDF',
                    className: 'btn btn-danger',
                    exportOptions: {
                        columns: 'th:not(:last-child)'
                    }
                },
            ]
        });
    });
    TableManageButtons.init();

</script>
<script>
    $(document).ready(function() {

        $("#datatable").on("click",
            ".loadModal3",
            function() {
                $("#id").val($(this).data('id'));
                $('#myModal1').modal('show');
            });
    });
</script>
<script>
    // Signout function
    function signOut() {
        const auth2 = gapi.auth2.getAuthInstance();
        auth2.signOut().then(function() {
            location.href = "@Url.Action("LogOut", "Account")";
        });

    }

    function renderButton() {
        gapi.load('auth2',
            function() {
                gapi.auth2.init();
            });
    }
</script>
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@8"></script>


@if (TempData["display"] != null)
{
    if (TempData["notificationtype"].Equals(NotificationType.Success.ToString()))
    {
        <script>
            Swal.fire({
                text: '@TempData["display"]',
                type: 'success'
            })
        </script>
    }
    if (TempData["notificationtype"].Equals(NotificationType.Error.ToString()))
    {
        <script>
            Swal.fire({
                text: '@TempData["display"]',
                type: 'error'
            })
        </script>
    }
    if (TempData["notificationtype"].Equals(NotificationType.Info.ToString()))
    {
        <script>
            Swal.fire({
                text: '@TempData["display"]',
                type: 'info'
            })
        </script>
    }
}

@RenderSection("Script", false)
</body>
</html>